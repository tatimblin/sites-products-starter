{
    "artifactStructure": {
        "assets": [
            {
                "root": "assets"
            },
            {
                "root": "static"
            }
        ],
        "plugins": [
            {
                "pluginName": "plugin-pagesUrl",
                "sourceFiles": [
                    {
                        "root": "plugins/pagesURL"
                    }
                ],
                "event": "ON_URL_CHANGE",
                "functionName": "setPagesUrl"
            }
        ],
        "hbsTemplates": [
            { 
                "root": "templates",
                "pattern": "*.hbs"
            },
            { 
                "root": "partials",
                "pattern": "*.hbs"
            }
        ]
    },
    "base_image_tag": "node-12.18",
    "dependencies": {
        "installDepsCmd": "echo install",
        "requiredFiles": []
    },
    "buildArtifacts": {
        "build_setup_cmd": "npm install",
        "build_cmd": "npm run build"
    },
    "livePreview": {
        "serve_setup_cmd": "echo setup",
        "serve_cmd": "echo serve"
    },
    "watch_cmd": "npm run watch"
}